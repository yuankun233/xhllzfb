          </view>
        </view>
        <view data-id="3" class="my_C_11" onTap="goOurder">
          <image class="img_3" src="https://www.xiaohulaile.com/wxcx/gou2.png">
          </image>
          <view>
            已完成
          </view>
          <view class="jiaobiao2" a:if="{{statusnum2 > 0}}">
            {{statusnum2}}
          </view>
        </view>
        <view data-id="0" class="my_C_11" onTap="goOurder">
          <image class="img_4" src="https://www.xiaohulaile.com/wxcx/gou1.png">
          </image>
          <view>
            全部订单
          </view>
        </view>
      </view>
      <view class="JKda">
        健康档案
      </view>
      <view class="jkda_1" onTap="goFile">
        <image class="jkda_11" src="https://www.xiaohulaile.com/wxcx/gou22.png">
        </image>
        <view class="jkda_12">
          健康档案必填
        </view>
        <image class="jkda_13" src="https://www.xiaohulaile.com/wxcx/gou21.png">
        </image>
        <view class="jkda_14">
          就诊人管理({{user.archive_num}})
