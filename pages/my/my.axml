          </view>
        </view>
        <view data-id="2" class="my_C_11" onTap="goOurder">
          <image class="img_2" src="https://www.xiaohulaile.com/wxcx/gou3.png">
          </image>
          <view>
            待服务
          </view>
          <view class="jiaobiao1" a:if="{{statusnum > 0}}">
