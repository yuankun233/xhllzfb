}
.wdMes image{
  width: 37rpx;
  height: 37rpx;
}
.wdMes view:nth-child(2){
  margin-left: 22rpx;
  width: 620rpx;
}
.wdMes:nth-child(2){
  padding-bottom: 20rpx;
  border-bottom:4rpx solid #0000001A ;
}
.pledge {
  width: 690rpx;
  height: 70rpx;
  background: #F4F4F4;
  border-radius: 8rpx;
  padding: 0rpx 26rpx 0rpx 26rpx;
  margin-left: 30rpx;
