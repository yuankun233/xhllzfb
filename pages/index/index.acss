}
.func{
  display: flex;
  width: 580rpx;
  margin-left: 84rpx;
  margin-top: 60rpx;
  text-align: center;
  justify-content: space-between;
}
.func view image{
  width: 124rpx;
  height: 100rpx;
}
.func view:nth-child(2){
  font-size: 24rpx;
  font-weight: 500;
  color: #000000;
}
.func view:nth-child(3){
  font-size: 20rpx;
  color: #999999;
  /* font-weight:lighter; */
}
.nurseTitle {
  font-size:36rpx ;
  color: #333333;
  font-weight:500 ;
  width: 690rpx;
  margin-top: 36rpx;
  margin-left: 30rpx;
  display: flex;
  justify-content: space-between;
}
.nurseTitle text:nth-child(2){
  font-size:24rpx ;
  color: #999999;
  font-weight: lighter;
  margin-top: 10rpx;
}
.zkNurse {
  margin-left: 30rpx;
  margin-top: 16rpx;
  display: flex;
  width: 690rpx;
  justify-content: space-between;
}
.zkNurse >view:nth-child(1){
  width: 330rpx;
  height: 330rpx;
  background: #F4F9FF;
  position: relative;
  
} 
.zkNurse >view:nth-child(1) image{
  width:100%;
  height:100%;
  background: #F4F9FF;
} 
.zkNurse >view:nth-child(2){
  width: 330rpx;
  height: 330rpx;
  position: relative;
}
.zkNurse >view:nth-child(2)>view{
  width:330rpx;
  height: 150rpx;
}
.zkNurse >view:nth-child(2) image{
  width: 100%;
  height: 100%;
}
.zkNurse >view:nth-child(2)>view:nth-child(2){
  margin-top: 30rpx;
}
.nurseTeam {
  margin-left: 30rpx;
  margin-top: 32rpx;
  display: flex;
  width: 690rpx;
  justify-content: space-between;
}
.nurseTeam>view {
  width: 330rpx;
  height: 150rpx;
  background: #F4F9FF;
  border-radius: 8rpx;
}
.nurseTeam view image{
  width: 100%;
  height: 100%;
}
.wdMes{
  margin-top: 36rpx;
  display: flex;
