.am-terms{display:flex;flex-direction:column;padding:8rpx;padding:var(--am-terms-padding,8rpx);width:100%;box-sizing:border-box}.am-terms .am-terms-header{display:flex;align-items:center;color:#333;color:var(--am-terms-content-color,#333);margin-bottom:40rpx;margin-bottom:var(--am-terms-content-margin,40rpx)}.am-terms .am-terms-header .am-checkbox{margin-right:16rpx;margin-right:var(--am-terms-checkbox-margin,16rpx)}.am-terms .am-terms-header .text{font-size:30rpx;font-size:var(--am-terms-content-font-size,30rpx);line-height:44rpx;line-height:var(--am-terms-content-line-height,44rpx);flex:1}.am-terms .am-terms-header .text .link{color:#1677ff;color:var(--am-terms-link-color,#1677ff);display:inline}.am-terms .am-terms-body{display:flex;flex-direction:column}.am-terms .am-terms-body .am-button{margin-bottom:24rpx;margin-bottom:var(--am-terms-button-margin,24rpx)}.am-terms .am-terms-footer{color:#333;color:var(--am-terms-desc-color,#333);font-size:22rpx;font-size:var(--am-terms-desc-font-size,22rpx);line-height:1.5;line-height:var(--am-terms-desc-line-height,1.5);margin-top:8rpx;margin-top:var(--am-terms-desc-margin,8rpx)}.am-terms .am-terms-footer .text .link{color:#1677ff;color:var(--am-terms-link-color,#1677ff);display:inline}.am-terms-fixed{position:fixed;z-index:2;bottom:0;left:0;right:0;background-color:#fff;background-color:var(--am-terms-background-color,#fff);padding:16rpx;padding:var(--am-terms-fixed-padding,16rpx);width:100%;box-sizing:border-box}