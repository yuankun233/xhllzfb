.am-vtabs{width:100vw;height:100vh;display:flex;flex-direction:row}.am-vtabs-bar{width:240rpx;overflow:hidden}.am-vtabs-bar-line{position:absolute;left:0;top:24rpx;bottom:24rpx;width:6rpx;overflow:hidden;font-size:0;border-radius:50vh}.am-vtabs-bar-line-radius{position:absolute;right:0;width:16rpx;height:16rpx;border-radius:50vh;overflow:hidden;font-size:0}.am-vtabs-bar-line-radius__top{top:-16rpx}.am-vtabs-bar-line-radius__bottom{bottom:-16rpx}.am-vtabs-bar-placehold{flex:1 1 0%}.am-vtabs-bar-content{display:flex;flex-direction:column;height:100%;width:240rpx}.am-vtabs-bar-content::-webkit-scrollbar{display:none}.am-vtabs-bar-tab{position:relative;box-sizing:border-box;width:240rpx;padding:24rpx 48rpx;text-align:center}.am-vtabs-bar-tab__hover{opacity:.6}.am-vtabs-bar-tab__active{font-size:30rpx;font-size:var(--am-vtabs-fontSize-active,30rpx)}.am-vtabs-bar-tab__same{font-size:26rpx;font-size:var(--am-vtabs-fontSize-same,26rpx)}.am-vtabs-bar-title{position:relative;display:inline-block}.am-vtabs-content-wrap{overflow:hidden;flex:1;height:100%;background-color:#fff;background-color:var(--am-vtabs-content-bgc,#fff)}.am-vtabs-slides{height:100%}.am-vtabs-slides.animate{transition-duration:.3s}