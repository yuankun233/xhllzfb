.am-message{background-color:#fff;background-color:var(--am-message-background,#fff);padding:64rpx 24rpx;padding:var(--am-message-padding-h,64rpx) var(--am-message-padding-v,24rpx);text-align:center;line-height:normal;box-sizing:border-box;position:relative}.am-message::after{content:'';position:absolute;background-color:#eee;display:block;top:auto;right:0;bottom:0;left:0;height:1px;transform:scaleY(.5)}.am-message .am-icon{display:block;line-height:1;line-height:var(--am-message-icon-lineheight,1);font-size:128rpx;font-size:var(--am-message-icon-size,128rpx)}.am-message .icon-success{color:#1677ff;color:var(--am-message-success-color,#1677ff)}.am-message .icon-info{color:#1677ff;color:var(--am-message-info-color,#1677ff)}.am-message .icon-waiting{color:#00b578;color:var(--am-message-waiting-color,#00b578)}.am-message .icon-warn{color:#ff8f1f;color:var(--am-message-warn-color,#ff8f1f)}.am-message .icon-cancel,.am-message .icon-fail{color:#ff3b30;color:var(--am-message-fail-color,#ff3b30)}.am-message-main{font-size:36rpx;font-size:var(--am-message-title-size,36rpx);margin-top:40rpx;margin-top:var(--am-message-title-margin,40rpx);color:#333;color:var(--am-message-title-color,#333)}.am-message-sub{font-size:26rpx;font-size:var(--am-message-subtitle-size,26rpx);margin-top:16rpx;margin-top:var(--am-message-subtitle-margin,16rpx);color:#999;color:var(--am-message-subtitle-color,#999)}.am-message-button-wrap{margin-top:40rpx;margin-top:var(--am-message-margintop,40rpx);padding:0 24rpx;padding:0 var(--am-message-button-margin,24rpx)}.am-message-button-wrap .a-button{margin-bottom:24rpx;margin-bottom:var(--am-message-button-margin,24rpx)}