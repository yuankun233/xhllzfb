.am-container{box-sizing:border-box;width:100%;border-radius:16rpx;border-radius:var(--am-container-corner-radius,16rpx);padding:24rpx 0;padding:var(--am-container-padding-v,24rpx) 0;background:#fff;background:var(--am-container-background,#fff)}.am-container-content-box{padding:0 24rpx;padding:0 var(--am-container-padding-h,24rpx)}.am-container-content{display:flex;flex-wrap:wrap}.am-container-content>:nth-child(1n+1){flex-grow:1;width:0;margin-left:16rpx;margin-left:var(--am-container-padding-item,16rpx);border-radius:8rpx;border-radius:var(--am-container-item-corner-radius,8rpx)}.am-container-content>:nth-child(1){margin-left:0}.am-container .am-container-content .am-container{flex:0 0 100%;margin-top:24rpx;margin-top:var(--am-container-padding-h,24rpx);margin-left:0;padding:0}.am-container-content-grid{display:flex}.am-container-content-grid>:nth-child(1){width:432rpx;border-radius:8rpx;border-radius:var(--am-container-item-corner-radius,8rpx)}.am-container-content-grid-sub{display:flex;box-sizing:border-box;flex-direction:column;padding-left:16rpx;padding-left:var(--am-container-padding-item,16rpx);flex-grow:1}.am-container-content-grid-sub>:nth-child(1n+1){flex-grow:1;margin-top:16rpx;margin-top:var(--am-container-padding-item,16rpx);border-radius:8rpx;border-radius:var(--am-container-item-corner-radius,8rpx)}.am-container-content-grid-sub>:nth-child(1){margin-top:0}