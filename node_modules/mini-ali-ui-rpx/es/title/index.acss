.am-title{display:flex;align-items:center;padding:0 24rpx 24rpx;padding:0 var(--am-title-paddingLR,24rpx) var(--am-title-paddingB,24rpx);font-size:30rpx;font-size:var(--am-title-text-fontSize,30rpx);color:#333;color:var(--am-title-text-color,#333)}.am-title-line{position:relative;margin-bottom:24rpx;margin-bottom:var(--am-title-marginBottom,24rpx)}.am-title-line::after{content:'';position:absolute;background-color:#eee;display:block;top:auto;right:0;bottom:0;left:0;height:1px;transform:scaleY(.5)}.am-title-icon{width:48rpx;width:var(--am-title-icon-width,48rpx);height:48rpx;height:var(--am-title-icon-height,48rpx);overflow:hidden;margin-right:16rpx;margin-right:var(--am-title-icon-marginRight,16rpx);background-position:center;background-repeat:no-repeat;background-size:100% auto}.am-title-text{flex:1;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;font-weight:700}.am-title-operation{width:48rpx;width:var(--am-operation-icon-width,48rpx);height:48rpx;height:var(--am-operation-icon-height,48rpx);overflow:hidden;text-align:center;line-height:48rpx;line-height:var(--am-title-operation-lineHeight,48rpx);margin-left:16rpx;margin-left:var(--am-title-operation-marginLeft,16rpx)}.am-title-operation-icon{font-size:36rpx;font-size:var(--am-title-operation-icon-size,36rpx);color:#ccc;color:var(--am-title-operation-icon-color,#ccc)}