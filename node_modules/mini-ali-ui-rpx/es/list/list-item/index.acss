.am-list-item{display:flex;align-items:center;background:#fff;background:var(--am-list-item-background,#fff)}.am-list-item-hover{filter:brightness(.9)}.am-list-item-radius{margin:0 24rpx;margin:0 var(--am-list-item-outside-spacing,24rpx);border-radius:16rpx;border-radius:var(--am-list-item-border-radius,16rpx)}.am-list-prefix{display:flex;align-items:center;padding:24rpx;padding:var(--am-list-item-padding,24rpx);padding-right:0}.am-list-supporting{margin-right:24rpx;margin-right:var(--am-list-item-padding,24rpx);color:#999;color:var(--am-list-item-content-subtitle-color,#999);font-size:30rpx;font-size:var(--am-list-item-subtitle-size,30rpx);display:flex}.am-list-thumb{width:30px;height:30px;margin-right:24rpx;margin-right:var(--am-list-item-padding,24rpx)}.am-list-line{position:relative;padding:24rpx;padding:var(--am-list-item-padding,24rpx);padding-left:0;flex:1;display:flex;align-items:center;align-self:stretch;overflow:hidden;max-width:100%}.am-list-line::after{content:'';position:absolute;background-color:#eee;display:block;top:auto;right:0;bottom:0;left:0;height:1px;transform:scaleY(.5)}.am-list-text-wrapper{margin-right:24rpx;margin-right:var(--am-list-item-padding,24rpx);text-overflow:ellipsis;overflow:hidden;white-space:nowrap}.am-list-text-wrapper__fix{flex:1}.am-list-content{flex:3;text-align:left;align-self:stretch;display:flex;flex-direction:column;justify-content:space-evenly;overflow:hidden;line-height:normal}.am-list-content-subtitle,.am-list-content-title{display:flex;align-items:center}.am-list-content-title{font-size:34rpx;font-size:var(--am-list-item-content-title-size,34rpx);color:#333;color:var(--am-list-item-content-color,#333);white-space:nowrap;text-overflow:ellipsis;overflow:hidden;display:flex}.am-list-content-subtitle{font-size:30rpx;font-size:var(--am-list-item-subtitle-size,30rpx);color:#999;color:var(--am-list-item-content-subtitle-color,#999)}.am-list-content-subtitle-small{font-size:26rpx;font-size:var(--am-list-item-content-subtitle-size-small,26rpx)}.am-list-extra{color:#999;color:var(--am-list-item-extra-color,#999);display:flex;justify-content:flex-end;align-self:stretch;align-items:center;margin-left:24rpx;margin-left:var(--am-list-item-padding,24rpx);flex:2;overflow:hidden}.am-list-primary{color:#333;color:var(--am-list-item-content-color,#333);font-size:34rpx;font-size:var(--am-list-item-content-title-size,34rpx)}.am-list-wrap .am-list-content,.am-list-wrap .am-list-extra{white-space:normal;word-break:break-all}.am-list-arrow{display:flex;align-items:center;margin-left:24rpx;margin-left:var(--am-list-item-arrow-margin,24rpx);height:44rpx;height:var(--am-list-item-arrow-size,44rpx);width:44rpx;width:var(--am-list-item-arrow-size,44rpx)}.am-list-item.last .am-list-line-bottom::after,.am-list-item.last .am-list-line::after{display:none}