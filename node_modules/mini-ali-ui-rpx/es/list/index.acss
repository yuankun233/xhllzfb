.am-list-footer,.am-list-header{display:flex;align-items:center;color:#999;color:var(--am-list-footer-header-color,#999);padding-left:24rpx;padding-left:var(--am-list-padding,24rpx);padding-right:24rpx;padding-right:var(--am-list-padding,24rpx)}.am-list-header{font-size:30rpx;font-size:var(--am-list-header-font-size,30rpx);line-height:normal;padding-top:24rpx;padding-top:var(--am-list-header-padding-top,24rpx);padding-bottom:16rpx;padding-bottom:var(--am-list-header-padding-bottom,16rpx)}.am-list-body{position:relative}.am-list-body::before{content:'';position:absolute;background-color:#eee;display:block;top:0;right:0;bottom:auto;left:0;height:1px;transform:scaleY(.5)}.am-list-body::after{content:'';position:absolute;background-color:#eee;display:block;top:auto;right:0;bottom:0;left:0;height:1px;transform:scaleY(.5)}.am-list-footer{font-size:26rpx;line-height:normal;padding-top:16rpx;padding-top:var(--am-list-footer-padding-top,16rpx);padding-bottom:24rpx;padding-bottom:var(--am-list-footer-padding-bottom,24rpx)}.am-list-load-more{display:flex;justify-content:center;align-items:center;background:#fff;background:var(--am-list-background,#fff);padding:24rpx;padding:var(--am-list-load-padding,24rpx);font-size:30rpx;font-size:var(--am-list-load-fontSize,30rpx);line-height:1.5;line-height:var(--am-list-load-lineHeight,1.5);color:#333;color:var(--am-list-load-more-color,#333)}.am-list-load-more-txt{padding:0 16rpx;padding:0 var(--am-list-load-txt-paddingLR,16rpx);color:#999;color:var(--am-list-load-txt-color,#999)}.am-list-load-over{display:block;padding:24rpx;padding:var(--am-list-load-over-padding,24rpx);font-size:22rpx;font-size:var(--am-list-load-over-fontSize,22rpx);text-align:center;color:#ccc;color:var(--am-list-load-over-color,#ccc);text-shadow:1px 1px #f0f0f0}