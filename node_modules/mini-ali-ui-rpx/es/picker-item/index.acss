.am-picker-item{display:flex;align-items:center;background:#fff;background:var(--am-picker-background,#fff);padding-left:24rpx;padding-left:var(--am-picker-padding-left,24rpx)}.am-picker-line{position:relative;flex:1;display:flex;align-items:center;padding-right:24rpx;padding-right:var(--am-picker-line-padding-right,24rpx);min-height:96rpx;min-height:var(--am-picker-line-min-height,96rpx);overflow:hidden}.am-picker-label{align-self:center;min-width:160rpx;min-width:var(--am-picker-label-min-width,160rpx);margin-right:2px;font-size:34rpx;font-size:var(--am-picker-label-font-size,34rpx);color:#333;color:var(--am-picker-label-color,#333)}.am-picker-wrapper{display:flex;flex:1;height:48rpx;height:var(--am-picker-wrapper-height,48rpx);align-items:center;padding-left:3px;overflow:hidden;font-size:34rpx;font-size:var(--am-picker-font-size,34rpx)}.am-picker-layer{display:flex;flex:1}.am-picker-layer-vertical{flex-direction:column;padding:24rpx 0;padding:var(--am-picker-vlabel-paddingTB,24rpx) 0}.am-picker-layer-vertical .am-picker-label{align-self:flex-start;font-size:30rpx;font-size:var(--am-picker-vlabel-font-size,30rpx);color:#999;color:var(--am-picker-vlabel-color,#999)}.am-picker-layer-vertical .am-picker-wrapper{padding-left:0}.am-picker-content{flex:1;display:flex;align-items:center;color:#333;color:var(--am-picker-label-color,#333);text-align:left;overflow:hidden;white-space:nowrap;padding:0}.am-picker-clear{display:flex}.am-picker-line-bottom::after{content:'';position:absolute;background-color:#eee;display:block;top:auto;right:0;bottom:0;left:0;height:1px;transform:scaleY(.5)}.am-picker-item.last .am-picker-line-bottom::after{display:none}.am-picker-content-placeholder{color:#ccc;color:var(--am-picker-placeholder-color,#ccc)}.am-picker-content-value{color:#333;color:var(--am-picker-content-value-color,#333)}.am-picker-arrow{display:block;margin-left:16rpx;margin-left:var(--am-picker-arrow-marginLeft,16rpx);line-height:1}