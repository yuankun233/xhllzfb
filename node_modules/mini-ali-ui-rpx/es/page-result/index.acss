.am-page-result{display:flex;flex-direction:column;background:#fff;background:var(--am-result-background,#fff);height:100vh;overflow:hidden}.am-page-result-pic{width:400rpx;height:400rpx;margin:calc(50vh - 400rpx) auto 0;background-position:center bottom;background-repeat:no-repeat;background-size:contain}.am-page-result-title{margin:40rpx auto 0;margin:var(--am-result-margin-vertical,40rpx) auto 0;max-width:calc(100vw - 96rpx);max-width:calc(100vw - var(--am-result-title-h-spacing,96rpx));overflow:hidden;text-overflow:ellipsis;white-space:nowrap;text-align:center;font-size:36rpx;font-size:var(--am-result-title-fontSize,36rpx);color:#333;color:var(--am-result-title-color,#333)}.am-page-result-brief{margin:0 auto;padding-top:24rpx;padding-top:var(--am-result-brief-margin-top,24rpx);font-size:26rpx;font-size:var(--am-result-brief-fontSize,26rpx);color:#999;color:var(--am-result-brief-color,#999);max-width:calc(100vw - 192rpx);overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-box-orient:vertical;-webkit-line-clamp:2;text-align:center}.am-page-result-button{text-align:center;margin:40rpx auto 0;margin:var(--am-result-margin-vertical,40rpx) auto 0}.am-page-result-button-right{margin-left:24rpx;margin-left:var(--am-result-button-margin-left,24rpx)}.am-local-page{height:100%;padding-bottom:40rpx;padding-bottom:var(--am-result-margin-vertical,40rpx)}.am-local-page .am-page-result-pic{width:200rpx;height:200rpx;margin:40rpx auto 0;margin:var(--am-result-margin-vertical,40rpx) auto 0;background-size:contain}.am-local-page .am-page-result-brief{font-size:30rpx;font-size:var(--am-result-local-brief-fontSize,30rpx)}.page-empty{background-image:url("https://gw.alipayobjects.com/mdn/rms_997765/afts/img/A*wKJQR6FwnMgAAAAAAAAAAABkARQnAQ")}.page-error{background-image:url("https://gw.alipayobjects.com/mdn/rms_997765/afts/img/A*J-gFS7ezs-0AAAAAAAAAAABkARQnAQ")}.page-network{background-image:url("https://gw.alipayobjects.com/mdn/rms_997765/afts/img/A*QQl_SJ8PNjwAAAAAAAAAAABkARQnAQ")}.page-busy{background-image:url("https://gw.alipayobjects.com/mdn/rms_997765/afts/img/A*4z6jTpc3lnQAAAAAAAAAAABkARQnAQ")}.page-logoff{background-image:url("https://gw.alipayobjects.com/mdn/rms_997765/afts/img/A*NplJSrfoAYYAAAAAAAAAAABkARQnAQ")}.page-payment{background-image:url("https://gw.alipayobjects.com/mdn/rms_997765/afts/img/A*utEARL8Wu0EAAAAAAAAAAABkARQnAQ")}.page-redpacket{background-image:url("https://gw.alipayobjects.com/mdn/rms_997765/afts/img/A*A1-PRJsm9_cAAAAAAAAAAABkARQnAQ")}