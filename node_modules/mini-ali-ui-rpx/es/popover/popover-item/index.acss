.am-popover-item{max-width:170px;padding:0 24rpx;padding:0 var(--am-popover-item-padding,24rpx)}.am-popover-item-hover{background-color:#f2f2f2}.am-popover-item-wrapper{position:relative;padding-top:24rpx;padding-top:var(--am-popover-item-padding,24rpx);padding-bottom:24rpx;padding-bottom:var(--am-popover-item-padding,24rpx);padding-right:72rpx;padding-right:var(--am-popover-item-padding-right,72rpx);font-size:30rpx;font-size:var(--am-popover-item-font-size,30rpx);white-space:nowrap;overflow:hidden;text-overflow:ellipsis;color:#333;color:var(--am-popover-item-font-color,#333);display:flex;align-items:center}.am-popover-item-icon-holder{height:44rpx;height:var(--am-popover-item-icon-size,44rpx);width:44rpx;width:var(--am-popover-item-icon-size,44rpx)}.am-popover-item-ver-center{display:flex;align-items:center}.am-popover-item-slot-holder{padding-left:24rpx;padding-left:var(--am-popover-item-icon-text-space,24rpx)}.am-popover-item:not(:last-child)>.am-popover-item-wrapper::after{content:'';position:absolute;background-color:#eee;display:block;top:auto;right:0;bottom:0;left:0;height:1px;transform:scaleY(.5)}